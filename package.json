{
  "name": "cyber-threat-intelligence-platform",
  "version": "1.0.0",
  "description": "AI-Powered Cyber Threat Intelligence Platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --report-unused-disable-directives",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "build:prod:domain": "cross-env VITE_API_BASE_URL=https://cyberintelai.co/api/v1 npm run build",
    "backend:dev": "cd backend && npm run dev",
    "backend:build": "cd backend && npm run build",
    "backend:start": "cd backend && npm start",
    "install:all": "npm install && cd backend && npm install",
    "dev:full": "concurrently \"npm run backend:dev\" \"npm run dev\"",
    "build:full": "npm run backend:build && npm run build",
    "start:full": "npm run backend:start"
  },
  "dependencies": {
